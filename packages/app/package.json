{
  "name": "@workspace/app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "npx rimraf .expo && npx cross-env APP_ENV=local npx expo start --clear",
    "android": "expo start --clear --android",
    "ios": "expo start --clear --ios",
    "build:web:dev": "npx rimraf .expo build dist web-build && npx cross-env APP_ENV=development GENERATE_SOURCEMAP=false npx expo export:web",
    "deploy:web:dev": "npm run build:web:dev && firebase use onyxsoftwaresolution-dev && firebase deploy --only hosting:onyxsoftwaresolution-dev",
    "build:web:prod": "npx rimraf .expo build dist web-build && npx cross-env APP_ENV=production GENERATE_SOURCEMAP=false npx expo export:web",
    "deploy:web:prod": "npm run build:web:prod && firebase use onyxsoftwaresolution && firebase deploy --only hosting:onyxsoftwaresolution",
    "web": "npx rimraf .expo && npx cross-env BROWSER=false expo start --clear --web"
  },
  "dependencies": {
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.20.7",
    "@babel/plugin-syntax-nullish-coalescing-operator": "^7.8.3",
    "@babel/preset-env": "^7.20.2",
    "@expo/webpack-config": "^0.17.2",
    "@react-native-community/datetimepicker": "6.5.2",
    "@react-navigation/bottom-tabs": "^6.5.6",
    "@react-navigation/drawer": "^6.6.1",
    "@react-navigation/material-top-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.5",
    "@react-navigation/native-stack": "^6.9.11",
    "@react-navigation/web": "^1.0.0-alpha.9",
    "@tanstack/react-query": "^4.24.10",
    "@workspace/api": "*",
    "class-validator": "^0.14.0",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "expo": "~47.0.12",
    "expo-constants": "~14.0.2",
    "expo-localization": "~14.0.0",
    "expo-secure-store": "~12.0.0",
    "expo-status-bar": "~1.4.2",
    "js-cookie": "^3.0.1",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-hook-form": "^7.43.2",
    "react-native": "0.70.5",
    "react-native-dotenv": "3.3.1",
    "react-native-gesture-handler": "~2.8.0",
    "react-native-paper": "^5.2.0",
    "react-native-paper-dates": "^0.12.0",
    "react-native-reanimated": "~2.12.0",
    "react-native-safe-area-context": "4.4.1",
    "react-native-screens": "~3.18.0",
    "react-native-tab-view": "^3.5.0",
    "react-native-vector-icons": "^9.2.0",
    "react-native-web": "~0.18.9",
    "rimraf": "^4.1.2"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.3",
    "@types/react": "~18.0.14",
    "@types/react-native": "~0.70.6",
    "@types/react-native-dotenv": "^0.2.0",
    "@types/react-native-vector-icons": "^6.4.13",
    "eslint-plugin-react-hooks": "^4.6.0",
    "typescript": "^4.6.3"
  },
  "private": true
}